{
	"name": "tutorial-with-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "deno run -A npm:vite & deno run server:start",
		"server:start": "deno run -A --watch ./api/main.ts",
		"server:production": "deno run -A ./api/main.ts",
		"build": "deno run -A npm:vite build",
		"preview": "deno run -A ./api/main.ts & deno run -A npm:vite preview"
	},
	"devDependencies": {
		"@deno/svelte-adapter": "^0.1.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	}
}
